: entity player
	   type          player
	   piece         player 0
	   animation     player 
	   mass          1.0
	   speed         70
#    speed 150
	   boundary_pill 0 0 0 3 8
	   collision_response PLAYER

: entity snail
   	type          enemy
	   piece         snail 0
	   animation     enemy
    animation_var STUN_TIME 3.0
	   mass          1.0  
	   speed         50  
	   boundary_circle 0 0 0 6
	   damage        2    
	   collision_response ENEMY
	   CAN_BE_STUNNED

: entity sally
   	type          enemy
	   piece         sally 0
	   animation     enemy
    animation_var STUN_TIME 4.0
